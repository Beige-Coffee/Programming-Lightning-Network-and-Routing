modules = ["python-3.11", "rust-stable"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run Rust"

[[workflows.workflow]]
name = "Run Rust"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cargo run"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80
